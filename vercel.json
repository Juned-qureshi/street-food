{
    "version": 2,
    "builds": [
        {
            "src": "poll/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "Python 3.10.8" } 
        },
        {
           "src": "build_files.sh",
           "use": "@vercel/static-build",
           "config": {
            "disDir": "staticfiles_build"
           }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "poll/wsgi.py"
        }
    ]
}